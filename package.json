{
  "name": "commitcounter",
  "version": "1.0.0",
  "description": "An API that generates commit count stats from Bitbucket and GitLab in SVG badge format.",
  "keywords": [
    "commitcounter",
    "bitbucket",
    "gitlab",
    "commits",
    "typescript",
    "api",
    "badge"
  ],
  "author": "Matheus Julidori",
  "license": "MIT",
  "homepage": "https://github.com/MatheusJulidori/commitstats-api#readme",
  "bugs": {
    "url": "https://github.com/MatheusJulidori/commitstats-api/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MatheusJulidori/commitstats-api.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@apollo/server": "^4.12.2",
    "axios": "^1.9.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "graphql": "^16.11.0",
    "xmlbuilder2": "^3.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/express": "^5.0.3",
    "@types/graphql": "^14.2.3",
    "@types/node": "^20.0.0",
    "eslint": "^9.28.0",
    "globals": "^16.2.0",
    "prettier": "^3.5.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.0"
  }
}
